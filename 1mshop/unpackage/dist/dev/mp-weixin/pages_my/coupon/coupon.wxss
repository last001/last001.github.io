}
.coupon .notUsed .u-popup__content .content ._h3 {
  color: #f04526;
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  top: 45%;
}
.coupon .notUsed .u-popup__content .content .text {
  font-size: 28rpx;
  color: #a4321f;
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  top: 53.5%;
}
.coupon .notUsed .u-popup__content .content .other {
  font-size: 24rpx;
  color: #999;
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  top: 62%;
}
.coupon .notUsed .u-popup__content .content .other text {
