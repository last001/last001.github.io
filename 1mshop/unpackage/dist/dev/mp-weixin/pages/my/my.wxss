.my .personInfo {
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
}
.my .personInfo .info .name {
  display: flex;
  align-items: center;
}
.my .personInfo .info .name .login {
  background-color: #fff;
  border: none;
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
  padding: 0;
  margin: 0;
}
.my .personInfo .info .name .login::after {
  content: "";
  width: 0;
  height: 0;
}
.my .personInfo .info .name .gender {
  width: 28rpx;
  height: 28rpx;
  margin-left: 20rpx;
  border: 1rpx solid red;
  box-sizing: border-box;
}
.my .personInfo .info .name .gender image {
  display: block;
  width: 100%;
  height: 100%;
}
.my .personInfo .info .phone {
  color: #ccc;
  font-size: 26rpx;
}
.my .personInfo .header {
  border: 1rpx solid red;
  box-sizing: border-box;
  width: 137rpx;
  height: 137rpx;
}
.my .personInfo .header image {
  display: block;
  width: 100%;
  height: 100%;
}
.my .welfare {
  display: flex;
  padding: 20rpx;
  padding-top: 0;
  justify-content: space-between;
}
.my .welfare .member {
  background: linear-gradient(to bottom, #FFF3C8, #FFFFFF);
  border-radius: 18rpx;
  padding: 20rpx;
  flex: 1;
  box-shadow: 0px 0px 9px 1px rgba(51, 42, 12, 0.05);
}
.my .welfare .member .v-member {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.my .welfare .member .v-member .member-name .text {
  color: #333;
  font-size: 28rpx;
}
.my .welfare .member .v-member .member-name .text-icon {
  color: #F8CF39;
  font-weight: bold;
  font-size: 22rpx;
  display: flex;
  padding: 20rpx 0;
}
.my .welfare .member .v-member .member-name .text-icon .u-icon--right {
  margin-left: 10rpx;
}
.my .welfare .member .v-member .menber-image {
  width: 40rpx;
  height: 40rpx;
  padding: 10rpx;
  background-color: #fff;
  border-radius: 50%;
}
.my .welfare .member .v-member .menber-image image {
  width: 100%;
  height: 100%;
}
.my .welfare .member .explain {
  width: 100%;
  color: #F8CF39;
  padding: 10rpx 0;
  border-radius: 18rpx;
  font-size: 19rpx;
}
.my .welfare .coupons {
  background: linear-gradient(to bottom, #FFE5E5, #FFFFFF);
  border-radius: 18rpx;
  padding: 20rpx;
  flex: 1;
  margin-left: 20rpx;
  box-shadow: 0px 0px 9px 1px rgba(51, 42, 12, 0.05);
}
.my .welfare .coupons .v-coupons {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.my .welfare .coupons .v-coupons .coupons-name .text {
  color: #333;
  font-size: 28rpx;
}
.my .welfare .coupons .v-coupons .coupons-name .text-icon {
  color: #FF4D4E;
  font-weight: bold;
  font-size: 22rpx;
  display: flex;
  padding: 20rpx 0;
}
.my .welfare .coupons .v-coupons .coupons-name .text-icon .u-icon--right {
  margin-left: 10rpx;
}
.my .welfare .coupons .v-coupons .coupons-image {
  width: 40rpx;
  height: 40rpx;
  padding: 10rpx;
  background-color: #fff;
  border-radius: 50%;
}
.my .welfare .coupons .v-coupons .coupons-image image {
  width: 100%;
  height: 100%;
}
.my .welfare .coupons .explain {
  width: 100%;
  color: #FFB4B4;
  padding: 10rpx 0;
  border-radius: 18rpx;
  font-size: 19rpx;
}
.my .list .lists .u-list {
  height: initial !important;
}
.my .list .lists .u-list .u-cell__body__content {
  position: relative;
}
.my .list .lists .u-list .u-cell__body__content .u-icon--right {
  position: absolute;
  right: 0;
  top: 8rpx;
}
.my .list .lists .u-list .u-line {
  width: 0% !important;
}

