<view class="printer"><view class="title"><block wx:for="{{titleList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['switchTitle',['$0','$1',index],['titleList',[['titleList','',index]]]]]]]}}" class="{{['v-title','flex-center-cs-r',item.isActive?'active':'']}}" bindtap="__e">{{''+item.name+''}}</view></block></view><view hidden="{{!(current==0)}}" class="printerList"><view class="printerText"><view class="v-printerText">打印列表</view></view><block wx:for="{{wifiList}}" wx:for-item="item" wx:for-index="index"><view>{{''+item+''}}</view></block><block wx:for="{{printerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list flex-l-r"><view class="radio-online"><view class="radio"><u-radio-group bind:input="__e" vue-id="{{'75ccf16c-1-'+index}}" value="{{radioVal}}" data-event-opts="{{[['^input',[['__set_model',['','radioVal','$event',[]]]]]]}}" bind:__l="__l" vue-slots="{{['default']}}"><u-radio vue-id="{{('75ccf16c-2-'+index)+','+('75ccf16c-1-'+index)}}" label-size="32" active-color="#F8CF39" name="{{item.name}}" data-event-opts="{{[['^change',[['radioChange']]]]}}" bind:change="__e" bind:__l="__l" vue-slots="{{['default']}}">{{''+item.name+''}}</u-radio></u-radio-group></view><view class="{{['online',item.isOnline?'active':'']}}">{{item.isOnline?'[在线]':'[离线]'}}</view></view><view class="btns"><view class="edit flex-center-cs-r"><u-icon vue-id="{{'75ccf16c-3-'+index}}" name="edit-pen" color="#999" size="28" bind:__l="__l"></u-icon><view class="text">编辑</view></view><view class="delete flex-center-cs-r"><u-icon vue-id="{{'75ccf16c-4-'+index}}" name="trash" color="#999" size="28" bind:__l="__l"></u-icon><view class="text">删除</view></view></view></view></block></view><view hidden="{{!(current==1)}}" class="bluetooth"><view class="switch"><view class="v-switch flex-l-r"><view class="text">附近的蓝牙设备</view><view class="openClose flex-center-cs-r"><text>开启/关闭蓝牙</text><u-switch vue-id="75ccf16c-5" inactive-color="#fdfdfd" active-color="#f8cf39" size="30" value="{{isSwitch}}" data-event-opts="{{[['^change',[['openBluetoothAdapter']]],['^input',[['__set_model',['','isSwitch','$event',[]]]]]]}}" bind:change="__e" bind:input="__e" bind:__l="__l"></u-switch></view></view></view><view hidden="{{!(isSwitch)}}" class="device"><view class="text">可用设备</view><view class="deviceList"><block wx:for="{{devices}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="v-deviceList"><view class="image"></view><view class="name">{{''+item.name+''}}</view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" data-event-params="{{({item})}}" class="state" bindtap="__e">{{item.isShowConnect?'已连接':'连接设备'}}</view></view></block></view></view></view></view>