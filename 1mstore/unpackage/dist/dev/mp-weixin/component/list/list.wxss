.list {
  background-color: #f2f2f2;
  padding: 20rpx 0;
}
.list .v-list {
  padding: 20rpx;
  background: #FFFFFF;
  box-shadow: 0px 3px 6px 1px rgba(0, 0, 0, 0.09);
}
.list .v-list:nth-of-type(n + 2) {
  margin-top: 20rpx;
}
.list .v-list .number {
  position: relative;
  left: -20rpx;
  width: calc(100% + 20rpx);
}
.list .v-list .number .count-text {
  display: flex;
  align-items: center;
}
.list .v-list .number .count-text .count {
  width: 126rpx;
  height: 66rpx;
  text-align: center;
  line-height: 66rpx;
  font-size: 34rpx;
  color: #fff;
  background-color: #02B6BF;
  border-top-right-radius: 52rpx;
  border-bottom-right-radius: 52rpx;
}
.list .v-list .number .count-text .text {
  font-weight: 400;
  font-size: 28rpx;
  color: #999999;
  margin-left: 20rpx;
}
.list .v-list .number .state {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.list .v-list .number .state .text {
  margin-right: 15rpx;
  font-weight: bold;
  color: #003333;
  font-size: 28rpx;
}
.list .v-list .number .state .u-checkbox-group .u-checkbox {
  display: block;
}
.list .v-list .info {
  width: 100%;
  margin: 20rpx 0;
}
.list .v-list .info .name-phone {
  display: flex;
}
.list .v-list .info .name-phone .name {
  display: flex;
  align-items: center;
}
.list .v-list .info .name-phone .name .v-name {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.list .v-list .info .name-phone .name .payNum {
  font-size: 22rpx;
}
.list .v-list .info .name-phone .name .payNum text {
  font-weight: 400;
  color: #FB2626;
}
.list .v-list .info .name-phone .phone {
  margin-left: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #02B6BF;
}
.list .v-list .info .notice {
  display: flex;
}
.list .v-list .info .notice .text {
  font-size: 28rpx;
  font-weight: 400;
  color: #FC9A26;
}
.list .v-list .address {
  font-size: 28rpx;
  font-weight: 400;
}
.list .v-list .productList {
  width: 100%;
  margin-top: 20rpx;
  padding: 20rpx 0;
  border-top: 2rpx dashed #EAEAEA;
}
.list .v-list .productList .text-time {
  display: flex;
  align-items: center;
}
.list .v-list .productList .text-time .text {
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
}
.list .v-list .productList .text-time .text .name {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.list .v-list .productList .text-time .text u-icon {
  display: block;
  margin-left: 20rpx;
}
.list .v-list .productList .text-time .text .u-icon {
  margin-left: 20rpx;
}
.list .v-list .productList .text-time .time {
  font-size: 26rpx;
  font-weight: 400;
  color: #FC8C53;
  margin-left: 15rpx;
}
.list .v-list .productList .open-close {
  font-size: 26rpx;
  font-weight: 400;
  color: #F8CF39;
}
.list .v-list .productList .open-close text {
  margin-right: 15rpx;
}
.list .v-list .isShow .setMeal:nth-child(2) {
  margin: 20rpx 0;
}
.list .v-list .isShow .setMeal .title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.list .v-list .isShow .setMeal .title text {
  margin-left: 20rpx;
}
.list .v-list .isShow .setMeal .price {
  display: flex;
  align-items: center;
}
.list .v-list .isShow .setMeal .price .old {
  font-size: 26rpx;
  font-weight: 400;
  color: #999999;
  margin-right: 15rpx;
  text-decoration: line-through;
}
.list .v-list .isShow .setMeal .price .new {
  font-size: 32rpx;
  font-weight: bold;
}
.list .v-list .isShow .showList:nth-of-type(n + 2) {
  margin: 20rpx 0;
}
.list .v-list .isShow .showList:last-child {
  margin: 20rpx 0;
}
.list .v-list .isShow .showList .text {
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.list .v-list .isShow .showList .price {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.list .v-list .isShow .showList .price text {
  font-size: 22rpx;
}
.list .v-list .isShow .total {
  padding: 20rpx 0;
  border-bottom: 2rpx dashed #EAEAEA;
}
.list .v-list .isShow .total .text {
  font-size: 26rpx;
  font-weight: bold;
}
.list .v-list .isShow .total .beCareful {
  margin-top: 20rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: #999;
}
.list .v-list .isShow .total .beCareful text {
  color: #333;
  margin-left: 10rpx;
}
.list .v-list .isShow .income {
  margin: 20rpx 0;
}
.list .v-list .isShow .income .text {
  font-size: 32rpx;
  font-weight: bold;
}
.list .v-list .isShow .income .price {
  font-size: 36rpx;
  font-weight: bold;
  color: #FB2626;
}
.list .v-list .isShow .remarks {
  margin: 20rpx 0;
  font-size: 26rpx;
  font-weight: 400;
  color: #FC6E26;
}
.list .v-list .s-refund {
  position: relative;
  left: -20rpx;
  width: calc(100% + 40rpx);
  padding: 20rpx;
  background: #FFFAE6;
  font-size: 28rpx;
}
.list .v-list .s-refund .reason text {
  color: #FC6E26;
}
.list .v-list .s-refund .money {
  margin-top: 20rpx;
}
.list .v-list .s-refund .money text {
  font-weight: 400;
  color: #FC6E26;
}
.list .v-list .btns {
  margin-top: 20rpx ;
}
.list .v-list .btns .u-btn {
  width: 150rpx;
}
.list .v-list .btns .u-btn:nth-of-type(n + 2) {
  margin-right: 20rpx;
}
.list .v-list .btns u-button:nth-of-type(n + 2) {
  margin-right: 20rpx;
}

